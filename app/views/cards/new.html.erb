<h1>New question</h1>
<div class="form">
<% form_for(@question) do |f| %>
  <%= f.error_messages %>
  <%= f.hidden_field :dictionary_id, :value => session[:dictionary_id] %>
    <%= f.label :question %><br />
    <%= f.text_field :question %>
  </p>
  <p>
    <%= f.label :answer %><br />
    <%= f.text_field :answer %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
   
<% end %>
</div>
<%= link_to 'Back', questions_path %>